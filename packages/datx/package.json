{
  "name": "datx",
  "version": "0.11.1",
  "description": "A MobX data store",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/datx.git"
  },
  "bugs": {
    "url": "https://github.com/infinum/datx/issues"
  },
  "homepage": "https://github.com/infinum/datx#readme",
  "author": "Infinum JavaScript Team <hello@infinum.co>",
  "license": "MIT",
  "devDependencies": {
    "@infinumjs/tslint-config": "^2.0.0-beta.5",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.0",
    "jest": "^22.4.3",
    "mobx": "^4.2.0",
    "rollup": "^0.58.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "mobx": "^4.1.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "benchmark": "jest benchmark",
    "benchmark-watch": "jest benchmark --watch",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "build": "rollup -c"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/",
      "../datx-utils/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "test/(.*).ts",
    "transform": {
      ".ts": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    }
  },
  "dependencies": {
    "datx-utils": "^0.11.0"
  }
}
